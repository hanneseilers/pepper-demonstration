import os
import time
import shutil

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)

    def onLoad(self):
        self.recordFolder = "/home/nao/recordings/cameras/"

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_onStart(self):
        if self.getParameter("useFTP"):
            self.copyFTP()
        else:
            self.copyLocal()

        self.onStopped()

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box

    def copyLocal(self):
        # get local app path
        url = self.behaviorAbsolutePath().split("apps/")
        if len(url) > 1:
            url = str( url[1].split("/")[0] ) + "/"
        url = "/data/home/nao/.local/share/PackageManager/apps/" + url

        self.logger.info("img src: " + self.recordFolder + "image.jpg")
        self.logger.info("img dst: " + url)

        shutil.copyfile(self.recordFolder + "image.jpg", url + "html/image.jpg")

    def copyFTP(self):
        session = ftplib.FTP()

        self.logger.info("connectin to ftp")
        session.connect(self.getParameter("url"))
        session.login(self.getParameter("user"), self.getParameter("password"))
        session.cwd('htdocs/tmp')

        self.logger.info("upload")
        try:
            session.delete("image.jpg")
        except:
            pass
        file = open(self.recordFolder + "image.jpg", 'rb')
        session.storbinary('STOR image.jpg', file)

        file.close()
        session.quit()]]></content></script><Input name="onLoad" type="1" type_size="1" nature="0" inner="1" tooltip="Signal sent when diagram is loaded." id="1" /><Input name="onStart" type="1" type_size="1" nature="2" inner="0" tooltip="Box behavior starts when a signal is received on this input." id="2" /><Input name="onStop" type="1" type_size="1" nature="3" inner="0" tooltip="Box behavior stops when a signal is received on this input." id="3" /><Output name="onStopped" type="1" type_size="1" nature="1" inner="0" tooltip="Signal sent when box behavior is finished." id="4" /><Parameter name="url" inherits_from_parent="0" content_type="3" value="" default_value="" custom_choice="0" tooltip="" id="5" /><Parameter name="user" inherits_from_parent="0" content_type="3" value="" default_value="" custom_choice="0" tooltip="" id="6" /><Parameter name="password" inherits_from_parent="0" content_type="3" value="" default_value="" custom_choice="0" password="1" tooltip="" id="7" /><Parameter name="useFTP" inherits_from_parent="0" content_type="0" value="0" default_value="1" tooltip="" id="8" /></Box><Box name="Wait For Signals" id="4" localization="8" tooltip="Wait for both inputs to be stimulated before stimulating its output." x="788" y="104"><bitmap>media/images/box/wait.png</bitmap><script language="4"><content><![CDATA[class MyClass(GeneratedClass):
